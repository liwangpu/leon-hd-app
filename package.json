{
  "name": "leon-angular-app-scaffold",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "e2e": "ng e2e",
    "kenn": "ng serve --configuration=kenn --host 0.0.0.0",
    "dev": "ng serve --configuration=development --host 0.0.0.0",
    "b-dev": "ng build --configuration=production --aot  --prod ",
    "b-prod": "ng build --configuration=production --aot  --prod ",
    "b-kenn": "ng build --configuration=kenn --aot  --prod ",
    "bas": "npm run build:ssr && npm run serve:ssr",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server.js",
    "build:client-and-server-bundles:dev": "ng build -c=development --aot --prod && ng run manage-site:server",
    "build:ssr:dev": "npm run build:client-and-server-bundles:dev && npm run webpack:server",
    "dev:build-and-serve": "npm run build:ssr:dev && npm run serve:ssr",
    "build:client-and-server-bundles:aliyun": "ng build --prod -c=aliyun && ng run manage-site:server",
    "build:ssr:aliyun": "npm run build:client-and-server-bundles:aliyun && npm run webpack:server",
    "aliyun:build-and-serve": "npm run build:ssr:aliyun && npm run serve:ssr"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.0.3",
    "@angular/cdk": "^7.0.3",
    "@angular/common": "~7.0.3",
    "@angular/compiler": "~7.0.3",
    "@angular/core": "~7.0.3",
    "@angular/flex-layout": "7.0.0-beta.19",
    "@angular/forms": "~7.0.3",
    "@angular/http": "~7.0.3",
    "@angular/material": "^7.0.3",
    "@angular/platform-browser": "~7.0.3",
    "@angular/platform-browser-dynamic": "~7.0.3",
    "@angular/router": "~7.0.3",
    "@angular/upgrade": "~7.0.3",
    "@nguniversal/express-engine": "~7.0.2",
    "@nguniversal/module-map-ngfactory-loader": "~7.0.2",
    "@ngx-translate/core": "10.0.2",
    "@ngx-translate/http-loader": "3.0.1",
    "angular2-hotkeys": "^2.1.4",
    "angularx-qrcode": "^1.5.1",
    "core-js": "~2.5.4",
    "crypto-js": "3.1.9-1",
    "file-saver": "1.3.8",
    "hammerjs": "~2.0.8",
    "material-design-icons": "3.0.1",
    "moment": "~2.13.0",
    "ng2-tree": "2.0.0-rc.11",
    "roboto-fontface": "~0.10.0",
    "rxjs": "~6.3.0",
    "rxjs-compat": "6.2.1",
    "ts-md5": "1.2.4",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.10.0",
    "@angular-devkit/build-ng-packagr": "~0.10.0",
    "@angular/cli": "^7.0.0",
    "@angular/compiler-cli": "^7.0.0",
    "@angular/platform-server": "^7.0.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "^2.0.4",
    "@types/node": "~8.9.4",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "lodash": "^4.16.2",
    "ng-packagr": "^4.2.0",
    "phantomjs-prebuilt": "^2.1.7",
    "protractor": "~5.4.0",
    "ts-loader": "^4.2.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.1",
    "webpack-cli": "^3.1.0"
  }
}
