{
  "name": "hd-app-site",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "e2e": "ng e2e",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:ssr": "node dist/server.js",
    "build:client-and-server-bundles": "ng build --prod && ng run hd-app-site:server",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "upload:docker:test": "docker stop hd-app-site-c && docker rm hd-app-site-c",
    "build:docker:image": "docker build -t apps-morejee-site:latest .",
    "run:docker:image": "docker run -p 4000:4000 -e APISERVER=http://kennapi.damaozhu.com.cn -e TOOLSERVER=http://kenntool.damaozhu.com.cn -e SECRETKEY=my-test-app-key --name hd-app-site-c apps-morejee-site:latest",
    "rebuild-and-run-docker-image": "npm run upload:docker:test && npm run build:docker:image && npm run run:docker:image",
    "build:prod:docker:image": "docker build -t omegaluo/app-site-morejee:norya ."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/cdk": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/flex-layout": "7.0.0-beta.19",
    "@angular/forms": "~7.2.0",
    "@angular/http": "~7.2.0",
    "@angular/material": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/pwa": "^0.7.0",
    "@angular/router": "~7.2.0",
    "@angular/service-worker": "~7.2.0",
    "@angular/upgrade": "~7.2.0",
    "@nguniversal/express-engine": "~7.0.2",
    "@nguniversal/module-map-ngfactory-loader": "~7.0.2",
    "@ngx-translate/core": "~10.0.2",
    "@ngx-translate/http-loader": "3.0.1",
    "angularx-qrcode": "~1.5.1",
    "core-js": "~2.5.4",
    "crypto-js": "3.1.9-1",
    "file-saver": "1.3.8",
    "hammerjs": "~2.0.8",
    "moment": "~2.13.0",
    "ng2-tree": "2.0.0-rc.11",
    "rxjs": "~6.3.0",
    "rxjs-compat": "6.2.1",
    "ts-md5": "1.2.4",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.12.0",
    "@angular/cli": "~7.2.0",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@angular/platform-server": "~7.2.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "child_process": "~1.0.2",
    "codelyzer": "~4.5.0",
    "dotenv": "~6.2.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "~0.2.2",
    "protractor": "~5.4.0",
    "ts-loader": "~5.3.3",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2",
    "webpack-cli": "~3.2.1"
  }
}
